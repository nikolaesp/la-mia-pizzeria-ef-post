<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Pizzas</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-GLhlTQ8iRABdZLl6O3oVMWSktQOp6b7In1Zl3/Jr59b6EGGoI1aFkw7cmDA6j6gD" crossorigin="anonymous">

</head>
<body>
    <nav class="navbar bg-body-tertiary">
        <form class="container-fluid justify-content-start">
            <a asp-area="" asp-controller="Pizza" asp-action="Index">
                <input type="button" class="btn btn-outline-success me-2" value="Index" />
            </a>
            <a asp-area="" asp-controller="Pizza" asp-action="Create">
                <input type="button" class="btn btn-outline-success me-2" value="Create" />
            </a>
        </form>
    </nav>
    <div class="container">
        @RenderBody()
        @if (IsSectionDefined("scripts"))
        {
            RenderSection("scripts");
        }
    </div>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js" integrity="sha384-w76AqPfDkMBDXo30jS1Sgez6pr3x5MlQ1ZAGC+nuZB+EYdgRZgiwxhTBTkF7CXvN" crossorigin="anonymous"></script>
</body>
</html>

